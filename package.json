{
  "name": "ts-types-validator",
  "version": "1.1.3",
  "description": "Typescript types validator",
  "main": "index.js",
  "scripts": {
    "prepare": "tsc",
    "fmt": "prettier --write \"**/*.{ts,js,json}\"",
    "lint": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GafurovNayl/ts-types-validator.git"
  },
  "bugs": {
    "url": "https://github.com/GafurovNayl/ts-types-validator/issues"
  },
  "keywords": [
    "TypeScript"
  ],
  "author": "Nayl Gafurov",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --verbose"
    }
  },
  "lint-staged": {
    "*.(ts|js|json)": [
      "pretty-quick --staged --pattern **/*.{ts,js,json} --verbose --bail"
    ],
    "compile/**/*.(ts|js)": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^3.9.5"
  },
  "homepage": "https://github.com/GafurovNayl/ts-types-validator",
  "files": [
    "index.js",
    "transformer.js",
    "compile/compile.js",
    "utils/loopFinder/Chain.js",
    "utils/loopFinder/ChainManager.js",
    "utils/operators/operators.js",
    "utils/operators/boolOperators.js"
  ]
}
